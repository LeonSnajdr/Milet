{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Serilog.AspNetCore": "Warning",
        "Samhammer.Options": "Warning",
        "Samhammer.DependencyInjection": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  },
  "CorsPolicyOptions": {
    "DomainUrls": ["*"]
  }
}
